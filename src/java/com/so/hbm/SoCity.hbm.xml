<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.so.pojo.SoCity" table="so_city">
        <id name="cityId" type="java.lang.Integer">
            <column name="city_id" />
            <generator class="native" />
        </id>        
        <property name="cityName">
            <column name="city_name" />
        </property>        
        <property name="dateCreated" type="timestamp">
            <column name="create_at" length="19" />
        </property>
        <property name="dateUpdated" type="timestamp">
            <column name="update_at" length="19" />
        </property>   
        <property name="createBy" column="create_by" />
        <property name="stateId" column="state_id" />
        <property name="updateBy" column="update_by" />     
    </class>
</hibernate-mapping>